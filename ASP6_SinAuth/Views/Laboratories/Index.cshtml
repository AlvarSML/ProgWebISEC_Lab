@model IEnumerable<ASP6_SinAuth.Models.Laboratory>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdLab">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.IdLab">Delete</a> |
                <a asp-action="TestTypes" asp-route-id="@item.IdLab">Offer</a> |
                <a asp-controller="LabWorkers" asp-action="" asp-route-id="@item.IdLab">Workers</a>
            </td>
        </tr>
}
    </tbody>
</table>
